, Button } from \"react-bootstrap\";\n\n\nconst Signup = () => {\n  const [userCategory, setUserCategory] = useState(1);\n  const [password, setPassword] = useState('');\n  const [passwordConfirm, setPasswordConfirm] = useState('');\n  const [redirection, setRedirection] = useState(false)\n  const [email, setEmail] = useState('');\n\n  const dispatch = useDispatch();\n\n  const data = {\n    user: {\n      email: email,\n      password: password\n    }\n  };\n\n  const handleSubmit = (e) => {\n    e.preventDefault()\n    fetch(\"https://dippr-api-development.herokuapp.com/api/signup\", {\n      \"method\": \"POST\",\n      \"headers\": {\n        \"Content-Type\": \"application/json\"\n      },\n      \"body\": JSON.stringify(data)\n    })\n    .then((response) => {\n      Cookies.set('token', response.headers.get(\"Authorization\"))\n      return response.json()\n    })\n    .then((response) => {\n      dispatch(loginUser(response.data.attributes))\n      console.log(loginUser(response.data.attributes))\n      setRedirection(true)\n    }).catch(error => {\n      console.log(error)\n    })\n  };\n\n\n  return (\n    <>\n      {redirection && <Redirect to='/'/>}\n      <Container fluid>\n        <Row>\n        <Col xs=\"3\"></Col>\n        <Col xs=\"6\">\n          <Form onSubmit={handleSubmit}>\n            <Form.Group controlId=\"email\">\n              <Form.Label>Email</Form.Label>\n              <Form.Control type=\"email\" placeholder=\"Email\" value={email} onChange={e => setEmail(e.target.value)}/>\n              <Form.Text className=\"text-muted\">\n                Nous ne partageons vos informations avec personne d'autre.\n              </Form.Text>\n            </Form.Group>\n            <Form.Group controlId=\"password\">\n              <Form.Label>Mot de passe</Form.Label>\n              <Form.Control type=\"password\" placeholder=\"Mot de passe\" value={password} onChange={e => setPassword(e.target.value)}/>\n            </Form.Group>\n            <Form.Group controlId=\"passwordConfirm\">\n              <Form.Label>Confirmer le mot de passe</Form.Label>\n              <Form.Control type=\"password\" placeholder=\"Confirmer le mot de passe\" value={passwordConfirm} onChange={e => setPasswordConfirm(e.target.value)}/>\n            </Form.Group>\n            <Button variant=\"primary\" type=\"submit\">\n              S'inscrire\n            </Button>\n          </Form>\n        </Col>\n        </Row>\n      </Container>\n    </>\n\n  );\n}\n\nexport default Signup\n",["192","193"],"/home/bakbledy93/NEXT/W11-12/Dippr/src/pages/Profile/index.jsx",[],"/home/bakbledy93/NEXT/W11-12/Dippr/src/pages/Home/index.jsx",[],"/home/bakbledy93/NEXT/W11-12/Dippr/src/components/Navbar/use-debounce.js",["194"],"/home/bakbledy93/NEXT/W11-12/Dippr/src/pages/Home/UserHome/index.jsx",[],"/home/bakbledy93/NEXT/W11-12/Dippr/src/serviceWorkerRegistration.js",[],"/home/bakbledy93/NEXT/W11-12/Dippr/src/reportWebVitals.js",[],"/home/bakbledy93/NEXT/W11-12/Dippr/src/components/UI/Stars/index.jsx",[],{"ruleId":"195","severity":1,"message":"196","line":13,"column":8,"nodeType":"197","messageId":"198","endLine":13,"endColumn":13},{"ruleId":"195","severity":1,"message":"199","line":33,"column":23,"nodeType":"197","messageId":"198","endLine":33,"endColumn":37},{"ruleId":"195","severity":1,"message":"200","line":39,"column":10,"nodeType":"197","messageId":"198","endLine":39,"endColumn":24},{"ruleId":"201","replacedBy":"202"},{"ruleId":"203","replacedBy":"204"},{"ruleId":"195","severity":1,"message":"205","line":36,"column":9,"nodeType":"197","messageId":"198","endLine":36,"endColumn":13},{"ruleId":"201","replacedBy":"206"},{"ruleId":"203","replacedBy":"207"},{"ruleId":"195","severity":1,"message":"208","line":3,"column":8,"nodeType":"197","messageId":"198","endLine":3,"endColumn":14},{"ruleId":"195","severity":1,"message":"209","line":9,"column":8,"nodeType":"197","messageId":"198","endLine":9,"endColumn":19},{"ruleId":"195","severity":1,"message":"2